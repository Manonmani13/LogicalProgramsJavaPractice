import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      int a[]={2,4,5,6};
      int k=2;
      for(int i=0;i<a.length;i++)
      {
        if(a[i]%k==0)
        {
          System.out.println(a[i]);
          break;
        }
      }
  }
}
