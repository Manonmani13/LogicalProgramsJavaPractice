import java.util.*;

public class Main {
    public static void main(String[] args) {
      int n[]={2,3,4,4,3,5};
      int count=1;
      String s="";
      for(int i=0;i<n.length;i++)
      {
        for(int j=i+1;j<=n.length-1;j++)
        {
          if(n[i]==n[j]){
            ++count;
            s=s+n[i]+",";
            System.out.println(n[i]+" "+count);
          }
          
        }
        count=1;
      }
      
            System.out.println(s);
  }
}
