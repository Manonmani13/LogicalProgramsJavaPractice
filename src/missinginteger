class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int n=(11*2)-1;
        int star=0;
        for(int j=0;j<=6*2-1;j++)
        {
            if(j<=11*2-1)
            {
                n=n-2;
                star++;
            }
            else
            
            {
                n=n+2;
                star--;
            }
        
        for(int i=1;i<=star;i++)
        {
            System.out.print("*");
        }
        for(int k=1;k<=n;k++)
        {
            System.out.print(" ");
        }
        for(int m=1;m<=star;m++)
        {
            if(m!=n)
            System.out.print("*");
        }
        System.out.println();
        }
    }
}
