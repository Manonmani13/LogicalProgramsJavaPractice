class HelloWorld {
   public static void main(String[] args) {
        int m=1,n=1;
        System.out.println("Try programiz.pro");
        for(int i=1;i<=11;i++)
        {
            for(int j=1;j<=m;j++)
            {
                System.out.print("*");
            }
           
           for(int j=m;j<=10;j++)
            {
                System.out.print(" ");
            }
             for(int j=1;j<=m;j++)
            {
                System.out.print("*");
            }
           
            
            if(i<=5){
                           m++;
                n+=2; 
            }
            else
            {
                            m--;

                 n+=2; 
            }
            System.out.println();
        }
}



}


