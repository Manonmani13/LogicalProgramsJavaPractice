import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      int nums[] ={131,48};
      String numbers="";
      int d=0;
      for(int i=0;i<nums.length;i++)
      {
        int no=nums[i];
        while(no!=0)
        {
          d=no%10;
          if(!numbers.contains(String.valueOf(d)))
          {
            numbers+=d+",";
          }
          no/=10;
          }
      }
        String argumnets[]=numbers.split(",");
        for(String arg:argumnets)
                System.out.println(arg);

  }
}
//odd even ascending

import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      int nums[] ={1, 2, 3, 5, 4, 7, 10,11,12};
      String oddno="",evenno="";
      int d=0;
      for(int i=0;i<nums.length;i++)
      {
       if(nums[i]%2!=0)
       {
        oddno+=nums[i];
       }
       else{
          evenno+=nums[i];

       }
       
      }
      int n=Integer.parseInt(oddno);
      while(n!=0)
      {
        d=n%10;
        System.out.println(d);
        n/=10;
      }
      System.out.println(evenno);


  }
}

//currency
import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      int nums[] ={2000, 500, 200, 100, 50, 20, 10, 5, 1} ;
      int no[]=new int[nums.length];
      int amount=800;
      for(int i=0;i<nums.length-1;i++)
      {
        if(amount>=nums[i])
        no[i]=amount/nums[i];
        amount%=nums[i];
       
      }
      for(int i=0;i<no.length-1;i++){
                if (no[i] != 0) {
      System.out.println(nums[i]);

                }
      }
      


  }
}
