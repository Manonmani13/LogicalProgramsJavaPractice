class HelloWorld {
   public static void main(String[] args) {
        int m=1,n=1;
        System.out.println("Try programiz.pro");
        for(int i=1;i<=11;i++)
        {
            for(int j=1;j<=m;j++)
            {
                System.out.print("*");
            }
           
           for(int j=m;j<=10;j++)
            {
                System.out.print(" ");
            }
             for(int j=1;j<=m;j++)
            {
                System.out.print("*");
            }
           
            
            if(i<=5){
                           m++;
                n+=2; 
            }
            else
            {
                            m--;

                 n+=2; 
            }
            System.out.println();
        }
}



}


// Online Java Compiler
// Use this editor to write, compile and run your Java code online

class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int n=(11*2)-1;
        int star=0;
        for(int j=0;j<=6*2-1;j++)
        {
            if(j<=11*2-1)
            {
                n=n-2;
                star++;
            }
            else
            
            {
                n=n+2;
                star--;
            }
        
        for(int i=1;i<=star;i++)
        {
            System.out.print("*");
        }
        for(int k=1;k<=n;k++)
        {
            System.out.print(" ");
        }
        for(int m=1;m<=star;m++)
        {
            if(m!=n)
            System.out.print("*");
        }
        System.out.println();
        }
    }
}

